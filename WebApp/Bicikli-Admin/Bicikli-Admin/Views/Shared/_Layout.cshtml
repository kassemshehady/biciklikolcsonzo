<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>@ViewBag.Title - Bicikli-Admin v1.0</title>
    @Styles.Render("~/Content/css", "~/Content/themes/base/css")
</head>
<body>
		<header>
			<h1>@Html.ActionLink("Bicikli-Admin v1.0", "Index", "Home")</h1>
		</header>
		
		<div id="menubar">
			@Html.Partial("_LoginPartial")
			@Html.Partial("_NavigationPartial");
		</div>
		
		<div id="page-body">
			@RenderBody()
		</div>
		
		<footer>
			<p>
				Design & Code by TakRaj
			</p>
		</footer>
		
		<!-- SCRIPTS -->
		@Scripts.Render("~/bundles/jquery")
		@Scripts.Render("~/Content/js")

        @RenderSection("scripts", required: false)
	</body>
</html>
